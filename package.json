{
  "name": "PomoPulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts --ext tsx --ext js --ext jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "clean:all": "rm -rf .next out node_modules pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.5.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "@eslint/eslintrc": "^3"
  }
}
